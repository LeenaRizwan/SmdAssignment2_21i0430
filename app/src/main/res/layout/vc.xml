<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DEF8F8"
    android:contentDescription="button"
    android:orientation="vertical"
    tools:context=".LogoPage1">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="550dp"
        android:layout_height="550dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-50dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/back5" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="John Cooper"
        android:textStyle="bold"
        android:textSize="30dp"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="100dp"></TextView>

    <TextView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="50:08"
        android:textStyle="bold"
        android:textSize="25dp"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="160dp"></TextView>

    <ImageView
        android:layout_width="90dp"
        android:contentDescription="image"
        android:layout_height="90dp"
        android:background="@drawable/vcicons3"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="150dp"></ImageView>

    <ImageView
        android:layout_width="350dp"
        android:contentDescription="image"
        android:layout_height="50dp"
        android:background="@drawable/vcicons1"
        android:layout_marginTop="520dp"
        android:layout_marginLeft="30dp"></ImageView>

    <ImageButton
        android:id="@+id/button87"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/vcicons2"
        android:layout_marginTop="580dp"
        android:contentDescription="Button"
        android:layout_marginLeft="180dp"></ImageButton>

</RelativeLayout>